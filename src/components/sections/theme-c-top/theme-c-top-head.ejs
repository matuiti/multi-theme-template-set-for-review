            /* 記事の公開日 - ISO 8601形式（YYYY-MM-DD） */
            "datePublished": "2025-05-05",

            /* 記事のURL - 実際に存在するURLを指定 */
            "url": "https://example.com/article/follow/1",

            /* 記事の説明 */
            "description": "この記事の概要文が入ります。",

            /* 記事の画像情報 */
            "image": {
              "@type": "ImageObject",
              "url": "https://example.com/images/follow/article1.jpg",
              "width": "1200",
              "height": "630"
            },

            /* 発行者情報 */
            "publisher": {
              "@type": "Organization",
              "name": "Curationテーマ公式サイト",
              "logo": {
                "@type": "ImageObject",
                "url": "https://example.com/images/logo.png",
                "width": "600",
                "height": "60"
              }
            }
          }
        },
        {
          "@type": "ListItem",
          "position": 2,
          "item": {
            "@type": "Article",
            "headline": "2番目の記事タイトルが入ります。",
            "author": {
              "@type": "Person",
              "name": "著者名"
            },
            "datePublished": "2025-05-04",
            "url": "https://example.com/article/follow/2",
            "description": "2番目の記事の概要文です。",
            "image": {
              "@type": "ImageObject",
              "url": "https://example.com/images/follow/article2.jpg",
              "width": "1200",
              "height": "630"
            },
            "publisher": {
              "@type": "Organization",
              "name": "Curationテーマ公式サイト",
              "logo": {
                "@type": "ImageObject",
                "url": "https://example.com/images/logo.png",
                "width": "600",
                "height": "60"
              }
            }
          }
        }
        /* 他の記事も同様の構造で追加
             各記事ごとにposition値を1ずつ増やし、
             実際のコンテンツに合わせて情報を更新する */
      ]
    }
  </script>

  <!-- Schema.org マークアップ - NEWセクション
